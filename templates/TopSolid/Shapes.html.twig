{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        {{ idobject }}
    </div>
    <div class="container " id="three" style="height: 800px">

    </div>
    <div id="data" style="display: none">
        {{  retour | json_encode   }}
    </div>
    <div>
        <button class="btn btn-secondary" onclick="afficherJson()">Afficher</button>
    </div>
    <div id="liste">

    </div>
    <script>
        function afficherJson() {
            const data = document.getElementById("data")
            if (data.style.display === "none"){
                data.style.display = "block"
            }
            else
            {
                data.style.display = "none"
            }
        }
    </script>
{% endblock %}

{% block javascripts %}

    {{ encore_entry_script_tags('testsTree') }}

    {{ encore_entry_script_tags('tests') }}
{% endblock %}

